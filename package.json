{
  "name": "imessage-client",
  "repository": {
    "type": "git",
    "url": "https://github.com/janicduplessis/imessage-client.git"
  },
  "main": "babel.server.js",
  "engines": {
    "iojs": "^2.3.0"
  },
  "scripts": {
    "start": "iojs ./babel.server",
    "build": "iojs ./node_modules/webpack/bin/webpack.js --verbose --colors --display-error-details --config webpack.client.js",
    "watch-client": "iojs ./node_modules/webpack/bin/webpack.js --verbose --colors --display-error-details --config webpack.client-watch.js && iojs ./node_modules/webpack-dev-server/bin/webpack-dev-server.js --config webpack.client-watch.js",
    "watch": "iojs ./node_modules/concurrently/src/main.js --kill-others \"npm run watch-client\" \"npm run start\"",
    "test": "jest"
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react",
      "<rootDir>/node_modules/react-tools"
    ],
    "testFileExtensions": [
      "es6",
      "js"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "es6"
    ]
  },
  "dependencies": {
    "babel": "^5.5.5",
    "babel-core": "^5.5.5",
    "babel-loader": "^5.1.0",
    "babel-runtime": "^5.5.5",
    "bcryptjs": "^2.1.0",
    "body-parser": "^1.12.4",
    "css-loader": "^0.14.2",
    "express": "^4.12.4",
    "express-jwt": "^3.0.1",
    "flummox": "^3.5.2",
    "immutable": "^3.7.2",
    "json-loader": "0.5.1",
    "jsonwebtoken": "^5.0.1",
    "material-ui": "^0.8.0",
    "morgan": "^1.5.3",
    "nconf": "^0.7.1",
    "node-sass": "^3.2.0",
    "react": "^0.13.3",
    "react-router": "^0.13.3",
    "react-style": "^0.5.5",
    "react-tap-event-plugin": "^0.1.7",
    "rethinkdb": "^2.0.0",
    "sass-loader": "^1.0.2",
    "socket.io": "^1.3.5",
    "socketio-jwt": "^4.0.1",
    "style-loader": "^0.12.3",
    "webpack": "^1.9.6"
  },
  "devDependencies": {
    "babel-eslint": "^3.1.14",
    "babel-jest": "^5.2.0",
    "concurrently": "0.0.5",
    "eslint": "^0.22.1",
    "eslint-plugin-react": "^2.3.0",
    "jest-cli": "^0.4.5",
    "react-hot-loader": "1.2.7",
    "react-tools": "^0.13.3",
    "webpack-dev-server": "^1.8.2"
  }
}
